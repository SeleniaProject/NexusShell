{
  "name": "nxsh-dark-default",
  "version": "1.0.0",
  "author": "NexusShell Team",
  "description": "Default dark theme with blue accents",
  "colors": {
    "primary": "#3b82f6",
    "secondary": "#6b7280",
    "accent": "#f59e0b",
    "background": "#1f2937",
    "foreground": "#f9fafb",
    "error": "#ef4444",
    "warning": "#f59e0b",
    "success": "#10b981",
    "info": "#3b82f6",
    "muted": "#6b7280",
    "highlight": "#fbbf24",
    "border": "#374151"
  },
  "styles": {
    "prompt": {
      "foreground": "Blue",
      "bold": true
