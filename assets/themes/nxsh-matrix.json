{
  "name": "nxsh-matrix",
  "version": "1.0.0",
  "author": "NexusShell Team",
  "description": "Green-on-black matrix style",
  "colors": {
    "primary": "#00ff41",
    "secondary": "#008f11",
    "accent": "#008000",
    "background": "#000000",
    "foreground": "#00ff41",
    "error": "#ff0000",
    "warning": "#ffff00",
    "success": "#00ff00",
    "info": "#00ff41",
    "muted": "#008f11",
    "highlight": "#39ff14",
    "border": "#003300"
  },
  "styles": {
    "prompt": {
      "foreground": "Blue",
      "bold": true
    },
    "command": {
      "foreground": "White"
    },
    "error": {
      "foreground": "Red",
      "bold": true
    },
    "warning": {
      "foreground": "Yellow"
    },
    "success": {
      "foreground": "Green",
      "bold": true
    }
  }
}